{
  "name": "@a16njs/plugin-a16n",
  "version": "0.5.1",
  "description": "a16n Intermediate Representation (IR) plugin for a16n",
  "license": "AGPL-3.0",
  "author": "Texarkanine",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Texarkanine/a16n.git",
    "directory": "packages/plugin-a16n"
  },
  "homepage": "https://texarkanine.github.io/a16n/plugin-a16n",
  "bugs": {
    "url": "https://github.com/Texarkanine/a16n/issues"
  },
  "keywords": [
    "a16n",
