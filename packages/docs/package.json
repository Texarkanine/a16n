{
  "name": "docs",
  "version": "0.3.0",
  "private": true,
  "description": "Documentation site for a16n",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "start:full": "npm run stage && npm run apidoc:current && docusaurus start",
    "build": "npm run stage && npm run apidoc:versioned && docusaurus build",
    "build:prose": "npm run stage && docusaurus build",
    "stage": "rm -rf .generated && mkdir -p .generated && cp -r docs/* .generated/",
    "apidoc:current": "npm run apidoc:current:engine && npm run apidoc:current:models && npm run apidoc:current:plugin-cursor && npm run apidoc:current:plugin-claude && npm run clidoc:current && npm run apidoc:current:postprocess",
    "apidoc:current:postprocess": "find .generated -type f -name 'README.md' -path '*/api/current/*' -execdir sh -c 'echo \"---\\ntitle: current (unreleased)\\nslug: /$(basename $(dirname $(dirname $(pwd))))/api/current\\n---\\n\" | cat - README.md > index.md && rm README.md' \\; && find .generated -type f -name '*.md' -path '*/api/current/*' -exec sed -i.bak 's|](../README.md)|](../)|g' {} + && find .generated -type f -name '*.md.bak' -path '*/api/current/*' -delete",
    "apidoc:current:engine": "typedoc --tsconfig ../engine/tsconfig.json --plugin typedoc-plugin-markdown --out .generated/engine/api/current ../engine/src/index.ts",
    "apidoc:current:models": "typedoc --tsconfig ../models/tsconfig.json --plugin typedoc-plugin-markdown --out .generated/models/api/current ../models/src/index.ts",
    "apidoc:current:plugin-cursor": "typedoc --tsconfig ../plugin-cursor/tsconfig.json --plugin typedoc-plugin-markdown --out .generated/plugin-cursor/api/current ../plugin-cursor/src/index.ts",
    "apidoc:current:plugin-claude": "typedoc --tsconfig ../plugin-claude/tsconfig.json --plugin typedoc-plugin-markdown --out .generated/plugin-claude/api/current ../plugin-claude/src/index.ts",
    "clidoc:current": "tsx scripts/generate-cli-docs.ts",
    "apidoc:versioned": "tsx scripts/generate-versioned-api.ts",
    "apidoc:versioned:dry-run": "tsx scripts/generate-versioned-api.ts --dry-run",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/faster": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@easyops-cn/docusaurus-search-local": "^0.52.0",
    "@types/node": "^20.0.0",
    "commander": "^12.0.0",
    "tsx": "^4.19.0",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.4.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
